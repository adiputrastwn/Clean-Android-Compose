# .env.default - Default environment variables for Fastlane
# This file serves as a template for environment-specific configuration
# Copy this file to .env and fill in your actual values
# IMPORTANT: Never commit .env file to version control!

# ========================================
# GOOGLE PLAY CONSOLE CREDENTIALS
# ========================================

# Path to your Google Play Console service account JSON key file
# You'll get this file from Google Cloud Console after setting up a service account
# Example: /Users/yourname/secrets/google-play-service-account.json
GOOGLE_PLAY_JSON_KEY_PATH=""

# Alternatively, you can paste the entire JSON content here (not recommended for security)
# SUPPLY_JSON_KEY_DATA=""

# ========================================
# APP SIGNING CONFIGURATION
# ========================================

# Path to your keystore file for signing release builds
# Example: /Users/yourname/keystores/release.jks
KEYSTORE_PATH=""

# Password for the keystore file
KEYSTORE_PASSWORD=""

# Key alias name in the keystore
KEY_ALIAS=""

# Password for the specific key
KEY_PASSWORD=""

# ========================================
# VERSIONING
# ========================================

# Override version name (optional)
# VERSION_NAME="1.0.0"

# Override version code (optional)
# VERSION_CODE="1"

# ========================================
# SLACK NOTIFICATIONS (optional)
# ========================================

# Slack webhook URL for build notifications
# SLACK_URL=""

# Slack channel to post notifications
# SLACK_CHANNEL="#mobile-builds"

# ========================================
# FIREBASE APP DISTRIBUTION (optional)
# ========================================

# Firebase App Distribution credentials file
# FIREBASE_APP_DISTRIBUTION_SERVICE_CREDENTIALS_FILE=""

# Firebase App ID
# FIREBASE_APP_ID=""

# Distribution groups
# FIREBASE_GROUPS="qa-team,developers"

# ========================================
# OTHER CONFIGURATION
# ========================================

# Skip waiting for build processing on Google Play (speeds up uploads)
# SUPPLY_SKIP_WAIT_FOR_BUILD_PROCESSING=true

# Timeout for Google Play upload (in seconds)
# SUPPLY_TIMEOUT=600